abstract class Shape {
    abstract void area();
}

class Rectangle extends Shape {
    int l = 5, b = 4;

    void area() {
        System.out.println("Rectangle Area: " + (l * b));
    }
}

class Circle extends Shape {
    double r = 3;

    void area() {
        System.out.println("Circle Area: " + (3.14 * r * r));
    }

    public static void main(String[] args) {
        Shape s;

        s = new Rectangle();
        s.area();

        s = new Circle();
        s.area();
    }
}
